
<!--轮播图-->
<swiper autoplay="{{autoplay}}" interval="{{interval}}" circular="{{circular}}" duration="{{duration}}" indicator-dots indicator-color="#f0f0f0" indicator-active-color="yellow">
  <swiper-item><image src="../../icons/swiper01.jpg"></image></swiper-item>
  <swiper-item><image src="../../icons/swiper02.jpg"></image></swiper-item>
  <swiper-item><image src="../../icons/swiper03.jpg"></image></swiper-item>
</swiper>

<!--两个标签-->
