page{background-color: #efeff4;}
swiper{
  width: 100%;
  height: 360rpx;
}

image{
  width: 100%;
  height: 100%;
}


.image_outer{
  display: flex;  /*拉伸容器*/
  flex-wrap: wrap;  /*元素换行*/
  padding: 10rpx;
  justify-content: space-between; /*水平方向的对齐，再平分空间*/
}
.image_inner{
  width: 48%;
  margin-bottom: 20rpx;
}
.image_inner image{
  width: 100%;
  border-radius: 15rpx;  /*图片的边框为圆角*/
}
.bangwoqu{
  padding: 30rpx;
}



.outer-view{
  display: flex;
  align-items: center;
  justify-content: center; 
}
.myborder{
  
  width: 100%;
  height: 300rpx;  /*每个框的高度*/
  border-color: #efeff4;    /*边框的颜色  */
  border-style: solid;
  border-width: 4rpx;
  border-radius: 25rpx;  /*圆角边框*/
  background-color: white;
  margin: 10rpx;  /*各边框距最外边的距离*/
}




