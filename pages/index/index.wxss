swiper{
  width: 100%;
  height: 450rpx;
}

image{
  width: 100%;
  height: 100%;
}


.image_outer{
  display: flex;  /*拉伸容器*/
  flex-wrap: wrap;  /*元素换行*/
  padding: 10rpx;
  justify-content: space-between; /*水平方向的对齐，再平分空间*/
}
.image_inner{
  width: 48%;
  margin-bottom: 20rpx;
}
.image_inner image{
  width: 100%;
  border-radius: 15rpx;  /*图片的边框为圆角*/
}
.bangwoqu{
  padding: 30rpx;
}






