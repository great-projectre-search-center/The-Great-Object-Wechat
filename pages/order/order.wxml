
<van-tabs active="{{ active }}" swipeable="{{swipeable}}" class="mytabs" animated>
  <van-tab title="全部订单">
  
    <view class="myorder">
      <van-card num="{{nums1}}" price="10.00" desc="全部订单111111111" title="全部订单1111111111"   >
        <view slot="footer">
          <van-button size="small" round>取消订单</van-button>
          <van-button size="small" round>确认订单</van-button>
        </view>
      </van-card>
    
  
      <!-- 分割线 -->
      <view class="divLine"></view>

      <van-card num="{{nums2}}" price="10.00" desc="全部订单222222222" title="全部订单222222222222" thumb="{{ imageURL }}">
        <view slot="footer">
          <van-button size="small" round>取消订单</van-button>
          <van-button size="small" round>确认订单</van-button>
        </view>
      </van-card>
      <!-- 分割线 -->
