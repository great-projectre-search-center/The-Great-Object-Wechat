
<van-tabs active="{{ active }}" swipeable="{{swipeable}}" class="mytabs" animated>
  <van-tab title="全部订单">
    <view wx:for="{{order1}}">


      <view class="outer-view">
        <!--订单框-->
        <view class="myborder">
          <!--订单1-->
          <view class="mycard">
            <navigator url="../orderDetail01/orderDetail01">
              <view wx:if="{{item.accepter_id}}=={{app.globalData.openid}}">
              <van-card
                num="{{nums1}}"
                tag="我的接单"
                desc="{{tiem.catalog}}"
                title="{{item.title}}"   
                >
              </van-card>
            </view>
            <view wx:else>
              <van-card
                num="{{nums1}}"
                desc="{{tiem.catalog}}"
                title="{{item.title}}"   
                >
              </van-card>
            </view>
            </navigator>
          </view>
          <!--按钮-->
          <view slot="footer" class="mybutton">
          <!-- 如果未接单 -->
            <van-button size="small" round>取消订单</van-button>
          <!-- 如果已接单 -->
            <van-button size="small" round>确认订单</van-button>
          <!-- 如果已确认 -->
          <!-- <van-button size="small" round>评价订单</van-button> -->
          </view>
        </view>
      </view>


    </view>

  </van-tab>


